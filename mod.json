{
	"geode": "4.2.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "omgrod.minecraftifycpa",
	"name": "Minecraftify: Custom Panoramas Addon",
	"version": "v1.0.0-alpha.2",
	"developer": "OmgRod",
	"description": "Adds custom panoramas to the Minecraftify mod",
	"dependencies": {
		"zalphalaneous.minecraft": ">=v0.9.14",
		"geode.node-ids": ">=v1.18.0"
 	},
	"resources": {
		"files": [
			"assets/panorama/thegardenawakens/*",
            "assets/panorama/trickytrials/*",
            "assets/panorama/trailsandtales/*",
            "assets/panorama/thewild/*",
			"assets/panorama/thenether/*",
			"assets/panorama/buzzybees/*",
			"assets/panorama/villageandpillage/*",
			"assets/panorama/gd/*"
		]
	},
	"settings": {
        "panorama": {
            "name": "Panorama",
            "type": "string",
            "default": "Tricky Trials",
            "one-of": ["Village & Pillage", "Buzzy Bees", "The Nether", "The Wild", "Trails & Tales", "Tricky Trials", "The Garden Awakens", "Geometry Dash"]
        }
    },
	"links": {
		"community": "https://discord.gg/vK3DuqJwyW",
		"homepage": "https://omgrod.me/",
		"source": "https://github.com/OmgRod/MinecraftifyCustomPanoramas"
	},
	"tags": ["content", "customization", "enhancement", "interface", "offline", "universal"],
	"early-load": true,
	"issues": {
		"info": "Report issues on the GitHub repository",
		"url": "https://github.com/OmgRod/MinecraftifyCustomPanoramas/issues"
	}
}
